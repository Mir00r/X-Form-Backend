    "googleapis": "^128.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "babel-jest": "^29.7.0",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "cross-env": "^7.0.3",
    "nyc": "^15.1.0",
    "sinon": "^17.0.1",
    "nock": "^13.4.0",
    "mongodb-memory-server": "^9.1.3",
    "redis-memory-server": "^0.10.0"
  },
  "optionalDependencies": {
    "ioredis": "^5.3.2",
    "amqplib": "^0.10.3",
    "ws": "^8.14.2",
    "elasticsearch": "^16.7.3",
    "prometheus-client": "^15.0.0"
  }
  "author": "X-Form Team",
  "license": "MIT"
}
