{
  "name": "response-service",
  "version": "1.0.0",
  "description": "Enhanced microservice for managing form responses with professional Swagger documentation",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "docker:build": "docker build -t response-service .",
    "docker:run": "docker run -p 3002:3002 response-service",
    "docs:generate": "swagger-jsdoc -d src/config/swagger.js -o docs/swagger.json src/routes/v1/*.js",
    "health": "curl http://localhost:3002/api/v1/health"
  },
  "keywords": [
    "microservice",
    "form-responses",
    "swagger",
    "openapi",
    "rest-api",
    "nodejs",
    "express"
  ],
  "author": "X-Form Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.2",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "express-validator": "^7.0.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.3.0",
    "mime-types": "^2.1.35",
    "sanitize-html": "^2.11.0",
    "validator": "^13.11.0",
    "dotenv": "^16.3.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0"
  }
}
